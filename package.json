{
  "name": "relayr-browser-sdk",
  "version": "2.0.0",
  "description": "The relayr browser sdk",
  "scripts": {
    "start": "node server.js",
    "test": "NODE_ENV=test mocha --recursive --compilers js:babel-core/register",
    "test:watch": "npm run test -- -w",
    "build:js": "webpack --config webpack.dev.config.js --progress --profile --colors",
    "build:min:js": "NODE_ENV=production webpack -p --config webpack.prod.config.js --progress --profile --colors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/relayr/browser-sdk"
  },
  "author": "Relayr (https://github.com/relayr)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/relayr/browser-sdk/issues"
  },
  "homepage": "https://github.com/relayr/browser-sdk",
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "chai": "^3.4.1",
    "es6-promise": "^3.0.2",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.6.2",
    "expect": "^1.20.1",
    "mocha": "^2.3.4",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1",
    "webpack-node-externals": "^1.2.0"
  },
  "dependencies": {
    "react": "^0.14.6",
    "react-dom": "^0.14.6"
  }
}
